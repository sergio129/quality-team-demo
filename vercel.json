{
  "version": 2,
  "buildCommand": "npx next build --no-lint",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "VERCEL_ENV": "production",
    "NEXT_TYPECHECK": "false",
    "NEXTAUTH_URL": "${URL}",
    "NODE_ENV": "production"
  },
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/auth/(.*)",
      "continue": true
    }
  ]
}
